{
  "name": "PRReport",
  "version": "0.0.14",
  "description": "PRReport will show pull request stats",
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffpriz/devops-pr-stats"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "postbuild": "npm run package-extension -- --rev-version",
    "package-extension": "tfx extension create --manifest-globs azure-devops-extension.json src/contributions/PRInfo/PRInfo.json",
    "publish-extension": "tfx extension publish --manifest-globs azure-devops-extension.json src/contributions/PRInfo/PRInfo.json"
  },
  "dependencies": {
    "azure-devops-extension-api": "~1.157.0",
    "azure-devops-extension-sdk": "~2.0.11",
    "azure-devops-ui": "2.166.98",
    "chart.js": "^2.9.4",
    "react": "~16.8.1",
    "react-chartjs-2": "^2.10.0",
    "react-dom": "~16.8.1",
    "react-scripts": "2.0.3"
  },
  "devDependencies": {
    "@types/react": "~16.8.2",
    "@types/react-dom": "~16.8.0",
    "@types/recharts": "^1.8.16",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "~1.0.0",
    "file-loader": "~2.0.0",
    "node-sass": "~4.12.0",
    "rimraf": "~2.6.2",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "tfx-cli": "^0.6.3",
    "ts-loader": "~5.2.2",
    "typescript": "^2.9.2",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2"
  }
}
